type DeliProduct{
    id: ID!
    products: [Product]
    saleId: [DeliSale]
    combinedWeight: Float
    deliProductPrice: Float
    deliProductQuantity: Int
    weightToPrice: Boolean
}

input DeliProductInput{
    productInputDtos: [ProductInputDto]
    productInputDto: ProductInputDto
    combinedWeight: Float
    portionType: PortionType

}

enum PortionType {
    SALAD
    FILLING
    QUANTITY
}